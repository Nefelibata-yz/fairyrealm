{
    "name": "fairyrealm",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "concurrently \"npm run dev --workspace=apps/web\" \"npm run dev --workspace=apps/worker\"",
        "build": "npm run build --workspaces --if-present",
        "test": "npm run test --workspaces --if-present"
    },
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "devDependencies": {
        "@cloudflare/workers-types": "^4.0.0",
        "concurrently": "^9.2.1",
        "typescript": "^5.0.0",
        "wrangler": "^4.56.0"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "dependencies": {
        "@opennextjs/cloudflare": "1.14.7",
        "next": "14.2.35"
    }
}